{
  "name": "node-uploads",
  "version": "1.0.0",
  "main": "index.js",
  "license": "MIT",
  "dependencies": {
    "body-parser": "^1.20.0",
    "dotenv": "^16.0.1",
    "ejs": "^3.1.7",
    "express": "^4.18.0",
    "multer": "^1.4.4"
  },
  "scripts": {
    "dev": "nodemon src/index.ts",
    "build": "tsc && mkdir -p ./dist/views/ && cp ./src/views/* ./dist/views/",
